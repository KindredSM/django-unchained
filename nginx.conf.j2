server {
    listen 80;
    server_name {{ domain }};

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root {{ app_dir }};
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/home/django_user/django-unchained/gunicorn.sock;
    }
}